<?xml version="1.0" encoding="UTF-8"?>

<form id="odnr16" name="Ohio Department of Natural Resources Stream Quality Monitoring Assessment">
    <meta>
        <formtype>Licking County Stream Team 2012</formtype>
        <formid>TEMPLATE</formid>
        <datecreated />
        <datemodified />
    </meta>
    <page name="PG 1">
        <section>
            <date name="Assessment Date" />
            <text name="River Name" />
            <text name="Mile Marker" />
        </section>
        <section name="Contact Information">
            <text name="Person/Group Name" />
            <number name="Number of Participants" />
            <text name="Leader Name" />
            <phone name="Phone" />
            <email name="Email Address" />
            <text name="Street Address" />
            <text name="City" />
            <text name="State" />
            <text name="ZIP Code" />
            <number name="Volunteer Hours" />
        </section>
        <section name="Report">
            <section>
                <text name="Water Conditions" lines="2" />
            </section>
            <section name="Total Suspended Solids (TSS Index)">
                <text name="Reading #1" />
                <text name="Reading #2" />
                <text name="Average" />
                <text name="TSS Index" hint="mg/l" />
            </section>
            <section name="Physical Measurements">
                <text name="Riffle Width" />
                <text name="Riffle Length" />
                <text name="Water Depth" />
                <text name="Water Temp." />
            </section>
            <section name="Bed Composition of Riffle">
                <number name="Silt" hint="%"/>
                <number name="Sand" hint="%" />
                <number name="Gravel (1/4 - 2&quot;)" hint="%"/>
                <number name="Cobble (2-10&quot;)" hint="%" />
                <number name="Boulder (>10&quot;)" hint="%" />
            </section>
            <section name="Macroinvertebrate Tally">
                <classifier groups="3" userdisplay="lettercodes" />
            </section>
        </section>
    </page>
</form>